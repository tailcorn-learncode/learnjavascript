from bs4 import BeautifulSoup
from locust import HttpUser, task, between

class AuthenticatedUser(HttpUser):
    wait_time = between(1, 5)

    @task
    def load_test(self):
        response = self.client.get("/home/dashboard")
        f = open("test.html", "w")
        f.write(response.text)
        f.close()
